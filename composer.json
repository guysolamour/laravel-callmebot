{
  "name": "guysolamour/laravel-callmebot",
  "description": "A package to send messages with callmebot api",
  "license": "MIT",
  "keywords": [
    "laravel",
    "callmebot"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Callmebot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Callmebot\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Callmebot\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
